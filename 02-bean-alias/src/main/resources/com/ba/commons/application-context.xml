<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="flipcart" class="com.ba.beans.Flipcart">
<property name="blCoriare" ref="bludart"/>
<property name="dtCoriare" ref="dtdc"/>
</bean>
<alias name="bludart" alias="dtdc"/>
<bean id="dtdc" class="com.ba.beans.DtdcCoriare"/>
<bean id="bludart" class="com.ba.beans.BludartCoriare"/>
</beans>
